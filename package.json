{
  "name": "@liamhelmer/claude-flow-ui",
  "version": "1.4.3",
  "description": "A modern web-based terminal and monitoring interface for Claude Flow with real-time system monitoring and tmux integration",
  "author": "Liam Helmer <liam@liamhelmer.com>",
  "license": "MIT",
  "homepage": "https://github.com/liamhelmer/claude-flow-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liamhelmer/claude-flow-ui.git"
  },
  "bugs": {
    "url": "https://github.com/liamhelmer/claude-flow-ui/issues"
  },
  "keywords": [
    "claude-flow",
    "terminal",
    "ui",
    "web-terminal",
    "monitoring",
    "tmux",
    "websocket",
    "real-time",
    "cli-tool",
    "developer-tools"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "main": "./unified-server.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./unified-server.js",
      "import": "./unified-server.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "unified-server.js",
    "websocket-server.js",
    ".next/**/*",
    "public/**/*",
    "src/app/**/*",
    "src/lib/**/*.js",
    "src/lib/**/*.ts",
    "src/services/**/*.ts",
    "src/services/**/*.js",
    "src/middleware/**/*.ts",
    "src/middleware/**/*.js",
    "src/types/**/*.ts",
    "next.config.js",
    "postcss.config.js",
    "tailwind.config.ts",
    "tsconfig.json",
    "types/",
    "README.md",
    "USAGE.md",
    "!**/__tests__/**",
    "!**/*.test.*",
    "!**/*.spec.*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "node unified-server.js",
    "build": "next build",
    "build:static": "NODE_ENV=production next build",
    "build:verify": "node scripts/build-verify.js",
    "test:integration": "node scripts/test-package-integration.js",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "diagnose": "node scripts/diagnose-tmux.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:enhanced": "jest --config=jest.config.coverage.js",
    "test:coverage:comprehensive": "node scripts/coverage/run-coverage.js --mode comprehensive",
    "test:coverage:quick": "node scripts/coverage/run-coverage.js --mode quick",
    "test:coverage:ci": "node scripts/coverage/run-coverage.js --mode ci",
    "test:coverage:collect": "node scripts/coverage/coverage-collector.js",
    "test:coverage:merge": "node scripts/coverage/coverage-merger.js",
    "test:coverage:analyze": "node scripts/coverage/coverage-analyzer.js",
    "test:coverage:report": "node scripts/coverage/coverage-reporter.js",
    "test:coverage:badges": "node scripts/coverage/coverage-reporter.js --badges-only",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:production": "node tests/regression/run-production-tests.js",
    "test:production-input": "NODE_ENV=production jest --config tests/regression/jest.production.config.js --testNamePattern=\"Input Display Delay\"",
    "test:production-switch": "NODE_ENV=production jest --config tests/regression/jest.production.config.js --testNamePattern=\"Terminal Switching\"",
    "test:production-ws": "NODE_ENV=production jest --config tests/regression/jest.production.config.js --testNamePattern=\"WebSocket\"",
    "test:production-all": "NODE_ENV=production jest --config tests/regression/jest.production.config.js",
    "test:terminal-input": "bash tests/regression/run-terminal-input-test.sh",
    "test:terminal-input-playwright": "npx playwright test --config=tests/playwright.config.regression.js tests/regression/terminal-input.spec.js",
    "test:performance": "ts-node tests/performance/run-performance-tests.ts",
    "test:performance:ci": "ts-node tests/performance/run-performance-tests.ts --mode ci",
    "test:performance:quick": "ts-node tests/performance/run-performance-tests.ts --mode quick",
    "test:performance:benchmarks": "ts-node tests/performance/run-performance-tests.ts --no-stress --no-bundle",
    "test:performance:stress": "ts-node tests/performance/run-performance-tests.ts --no-benchmarks --no-bundle",
    "test:performance:bundle": "ts-node tests/performance/run-performance-tests.ts --no-benchmarks --no-stress",
    "test:performance:k6": "node tests/performance/k6/scripts/run-k6-performance-tests.js",
    "test:performance:k6:setup": "node tests/performance/k6/setup.js",
    "test:performance:k6:smoke": "node tests/performance/k6/scripts/run-k6-performance-tests.js smoke",
    "test:performance:k6:load": "node tests/performance/k6/scripts/run-k6-performance-tests.js load",
    "test:performance:k6:stress": "node tests/performance/k6/scripts/run-k6-performance-tests.js stress",
    "test:performance:k6:spike": "node tests/performance/k6/scripts/run-k6-performance-tests.js spike",
    "test:performance:k6:soak": "node tests/performance/k6/scripts/run-k6-performance-tests.js soak",
    "test:performance:k6:comprehensive": "node tests/performance/k6/scripts/run-k6-performance-tests.js comprehensive",
    "test:performance:k6:parallel": "node tests/performance/k6/scripts/run-k6-performance-tests.js load --parallel",
    "lighthouse:ci": "lhci autorun --config=tests/performance/lighthouse/lighthouse-config.js",
    "lighthouse:desktop": "lighthouse http://localhost:3000 --preset=desktop --output=html --output-path=tests/performance/lighthouse/reports/desktop-report.html",
    "lighthouse:mobile": "lighthouse http://localhost:3000 --preset=mobile --output=html --output-path=tests/performance/lighthouse/reports/mobile-report.html",
    "test:e2e": "playwright test --config=tests/e2e/playwright.config.ts",
    "test:e2e:headed": "playwright test --config=tests/e2e/playwright.config.ts --headed",
    "test:e2e:ui": "playwright test --config=tests/e2e/playwright.config.ts --ui",
    "test:e2e:chromium": "playwright test --config=tests/e2e/playwright.config.ts --project=chromium",
    "test:e2e:firefox": "playwright test --config=tests/e2e/playwright.config.ts --project=firefox",
    "test:e2e:webkit": "playwright test --config=tests/e2e/playwright.config.ts --project=webkit",
    "test:e2e:mobile": "playwright test --config=tests/e2e/playwright.config.ts --project=mobile-chrome --project=mobile-safari",
    "test:e2e:visual": "playwright test --config=tests/e2e/playwright.config.ts --project=visual",
    "test:e2e:performance": "playwright test --config=tests/e2e/playwright.config.ts --project=performance",
    "test:e2e:accessibility": "playwright test --config=tests/e2e/playwright.config.ts --project=accessibility",
    "test:e2e:backstage": "playwright test --config=tests/e2e/playwright.config.ts tests/e2e/backstage-integration-workflows.spec.ts",
    "test:e2e:terminal": "playwright test --config=tests/e2e/playwright.config.ts tests/e2e/terminal/",
    "test:e2e:websocket": "playwright test --config=tests/e2e/playwright.config.ts tests/e2e/websocket/",
    "test:e2e:scrollback": "playwright test --config=tests/e2e/playwright.config.ts tests/e2e/scrollback/",
    "test:e2e:api": "playwright test --config=tests/e2e/playwright.config.ts tests/e2e/api/",
    "test:e2e:debug": "playwright test --config=tests/e2e/playwright.config.ts --debug",
    "test:e2e:report": "playwright show-report tests/e2e/reports/html",
    "test:e2e:install": "playwright install --with-deps",
    "test:e2e:codegen": "playwright codegen http://localhost:11235",
    "server": "node unified-server.js",
    "server:dev": "PORT=8080 node unified-server.js",
    "dev:all": "npm run dev & npm run server:dev",
    "prepublishOnly": "echo 'Preparing package for publish...' && npm run lint && npm run build:static",
    "prepack": "echo 'Packing package files...' && npm run build:static",
    "pack:test": "npm pack --dry-run",
    "pack:local": "npm pack",
    "publish:dry": "npm publish --dry-run",
    "claude-flow-ui": "node unified-server.js",
    "test:security": "jest --testPathPattern=tests/security --coverage",
    "test:security:auth": "jest tests/security/auth-security.test.ts",
    "test:security:input": "jest tests/security/input-validation.test.ts",
    "test:security:api": "jest tests/security/api-security.test.ts",
    "test:security:websocket": "jest tests/security/websocket-security.test.ts",
    "test:security:env": "jest tests/security/environment-security.test.ts",
    "test:security:owasp": "jest tests/security/owasp-compliance.test.ts",
    "test:security:scanner": "jest tests/security/security-scanner.test.ts",
    "test:security:cicd": "jest tests/security/security-cicd.test.ts",
    "test:security:all": "jest --testPathPattern=tests/security --coverage --verbose",
    "security:audit": "npm audit && npm audit --audit-level moderate",
    "security:scan": "node scripts/security/run-security-scan.js",
    "security:report": "node scripts/security/generate-security-report.js"
  },
  "bin": {
    "claude-flow-ui": "./unified-server.js"
  },
  "dependencies": {
    "@types/node": "^22.8.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@xterm/addon-canvas": "^0.7.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-search": "^0.15.0",
    "@xterm/addon-serialize": "^0.13.0",
    "@xterm/addon-unicode11": "^0.8.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "autoprefixer": "^10.4.20",
    "claude-flow": "^2.0.0-alpha.113",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "lucide-react": "^0.541.0",
    "next": "^15.5.0",
    "node-fetch": "^2.7.0",
    "node-pty": "^1.0.0",
    "postcss": "^8.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-tabs": "^6.0.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.0",
    "unique-filename": "^4.0.0",
    "uuid": "^13.0.0",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/typography": "^0.5.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node-fetch": "^2.6.13",
    "@types/react-tabs": "^2.3.4",
    "@types/react-test-renderer": "^19.1.0",
    "@types/validator": "^13.12.2",
    "@types/ws": "^8.5.12",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.5.0",
    "isomorphic-dompurify": "^2.21.0",
    "jest": "^30.0.5",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "lighthouse": "^12.0.0",
    "playwright": "^1.55.0",
    "react-test-renderer": "^18.3.1",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "validator": "^13.12.0",
    "ws": "^8.18.0"
  }
}
