# Claude Flow UI Environment Variables

# WebSocket Configuration
# The WebSocket port defaults to UI port + 1
# Only set these if you need to override the defaults
NEXT_PUBLIC_WS_PORT=11236
NEXT_PUBLIC_WS_URL=ws://localhost:11236

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:11235

# Claude Flow Configuration (if running integrated)
CLAUDE_API_KEY=your-api-key-here
CLAUDE_MODEL=claude-3-opus-20240229

# UI Configuration
NEXT_PUBLIC_THEME=dark
NEXT_PUBLIC_TERMINAL_FONT_SIZE=14
NEXT_PUBLIC_SIDEBAR_OPEN=true
NEXT_PUBLIC_MONITORING_OPEN=true